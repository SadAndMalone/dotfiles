urxvt*font:             xft:Bitstream Vera Sans Mono:size=10
urxvt*termName:         rxvt
urxvt*scrollBar:        false
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher
urxvt*urlLauncher:      /usr/bin/jumanji
urxvt*matcher.button:   1

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            full

 ! special
 *.foreground:   #f3f4f5
 *.background:   #6d6d6d
 *.cursorColor:  #f3f4f5

 ! black
 *.color0:       #282a2e
 *.color8:       #373b41

 ! red
 *.color1:       #85677b
 *.color9:       #c597d7

 ! green
 *.color2:       #ceb0d6
 *.color10:      #d2c4d6

 ! yellow
 *.color3:       #f7b1af
 *.color11:      #f7cac9

 ! blue
 *.color4:       #7193e2
 *.color12:      #92a8d1

 ! magenta
 *.color5:       #749f66
 *.color13:      #a8bba2

 ! cyan
 *.color6:       #fbc0a7
 *.color14:      #fbd8c9

 ! white
 *.color7:       #bdbebf
 *.color15:      #f3f4f5
 
